  <% @issues.keys.each do |series| %>
      <li class="cover_box <%= series.publisher.name.gsub(" ", "_")%> ">
        <div class="anchor ">
        <%= image_tag @issues[series].cover.url(:small), {:class => "home_comic_cover"} %>
        </div>
        <p class="issue_text"> <%= @issues[series].title.titleize  %></p>
      </li>
   <%end%>  <%#end of issues block%>
